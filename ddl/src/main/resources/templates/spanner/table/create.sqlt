[# th:if="${interleavedTable} == null or ${interleavedTable} == ''"]
CREATE TABLE [(${tableName})]([(${tableCode})]) [(${primaryKeyDefinition})];
[/]
[# th:if="${interleavedTable} != null and ${interleavedTable} != ''"]
CREATE TABLE [(${tableName})]([(${tableCode})]) [(${primaryKeyDefinition})],
INTERLEAVE IN PARENT [(${interleavedTable})] [(${deleteRule})];
[/]